{
    "nodes": [
        {
            "name": "const",
            "type": "ConstSource",
            "args": {
                "const": {"foo": 123, "hoge": "hoi"},
                "interval": 1
            },
            "to": ["docker"]
        },
        {
            "name": "docker",
            "type": "DockerTransformer",
            "args": {
                "repo": "alpine",
                "cmd": ["ls", "-la", "/tmp"]
            },
            "to": ["debug"]
        },
        {
            "name": "debug",
            "type": "DebugSink"
        }
    ]
}
